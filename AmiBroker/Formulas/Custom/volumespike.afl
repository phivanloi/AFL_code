AND C>=1
AND C*V >=3000000
AND C>1.02*Ref(C,-1)
AND C>=MA(C,30) 
AND MA(V,30)>=50000
AND C>=O
AND Ref(V,-5)>50000
AND Ref(V,-10)>50000
 ;

AddColumn(((C - Ref(C,-1))/Ref(C,-1))*100 ,"% Tang gia ");
